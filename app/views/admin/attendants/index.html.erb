<table>
  <thead>
    <tr>
      <th>No.</th>
      <th>Name</th>
      <th>E-mail</th>
      <th>Attended</th>
    </tr>
  </thead>
  <tbody>
    <% @attendants.each do |attendant| %>
    <tr>
      <td><%= attendant.sequence_number %></td>
      <td><%= attendant.name %></td>
      <td><%= attendant.email %></td>
      <td></td>
    </tr>
    <% end %>
  </tbody>
</table>
